{
  "name": "investing-snippets-blog",
  "version": "1.0.0",
  "description": "Next.js based InvestingSnippets blog.",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm install && next build && npm run build:rss && npm run build:sitemap",
    "build:rss": "node ./.next/server/scripts/generate-rss.js",
    "build:sitemap": "node ./.next/server/scripts/generate-sitemap.js",
    "start": "next start",
    "start:out": "http-server ./out -p 3000",
    "export": "next export && touch out/.nojekyll && cp out/_next/static/feed.xml out/ && cp out/_next/static/sitemap.xml out/",
    "deploy": "rm -rf node_modules/.cache/gh-pages && npm run build && npm run export && ./node_modules/.bin/gh-pages --dotfiles -d out -b gh-pages",
    "lint": "eslint ./components/** ./pages/** ./utils/** ./scripts/** -c .eslintrc.json --ext js,jsx",
    "lint-fix": "eslint ./components/** ./pages/** ./utils/** ./scripts/** -c .eslintrc.json --fix --ext js,jsx"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@matejmazur/react-katex": "^3.1.3",
    "@tailwindcss/typography": "^0.2.0",
    "clsx": "^1.1.1",
    "disqus-react": "^1.0.10",
    "gray-matter": "^4.0.2",
    "image-trace-loader": "^1.0.2",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^8.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jimp": "^0.16.1",
    "katex": "^0.11.1",
    "lazysizes": "^5.2.2",
    "lqip-loader": "^2.2.1",
    "next": "^10.0.3",
    "next-compose-plugins": "^2.2.0",
    "next-optimized-images": "^2.6.2",
    "next-transpile-modules": "^6.2.0",
    "nextjs-sitemap-generator": "^1.1.1",
    "postcss-preset-env": "^6.7.0",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-markdown": "^5.0.3",
    "react-syntax-highlighter": "^13.5.1",
    "remark-gfm": "^1.0.0",
    "remark-math": "^4.0.0",
    "rss": "^1.2.2",
    "svg-sprite-loader": "^5.0.0",
    "tailwindcss": "^1.7.6",
    "typeface-merriweather": "^0.0.72",
    "typeface-open-sans": "^0.0.75",
    "webp-loader": "^0.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.0.0",
    "gh-pages": "^3.1.0",
    "http-server": "^0.12.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/investingsnippets/investingsnippets.github.io.git"
  },
  "homepage": "https://github.com/investingsnippets/investingsnippets.github.io#readme",
  "keywords": [
    "next.js",
    "blog",
    "investingsnippets"
  ]
}
